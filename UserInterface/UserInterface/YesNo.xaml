            <ColumnDefinition x:Uid="ColumnDefinition_2" Width="92.373*" />
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition x:Uid="RowDefinition_1" Height="17.16*" />
            <RowDefinition x:Uid="RowDefinition_2" Height="42.84*" />
        </Grid.RowDefinitions>
